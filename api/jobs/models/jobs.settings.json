      "type": "string"
    },
    "price": {
      "type": "string"
    },
    "currency": {
      "type": "string"
    },
    "limitdate": {
      "type": "string"
    },
    "realisation_date": {
      "type": "string"
    },
    "experience": {
      "type": "string"
    },
    "valided": {
      "type": "boolean"
    },
    "taches": {
      "type": "json"
    },
    "freelancers": {
      "type": "json"
    },
    "typeofwork": {
      "type": "string"
    },
