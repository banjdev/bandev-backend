{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "contractants",
  "info": {
    "name": "contractants"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "compagnie": {
      "type": "string"
    },
    "responsable": {
      "type": "string"
    },
    "telephone": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "email": {
      "type": "email",
      "required": true,
      "unique": true
    },
    "password": {
      "type": "password"
    },
    "address": {
      "type": "string"
    },
    "isadmin": {
      "type": "boolean",
      "default": false
    },
    "code": {
      "type": "integer"
    },
    "validated": {
      "type": "boolean",
      "default": false,
      "private": false,
      "required": true
    },
    "code_expiration": {
      "type": "biginteger"
    }
  }
}
